{
  "info": { "name": "Example Collection", "_postman_id": "example", "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json" },
  "item": [
    {
      "name": "Get Root",
      "request": {
        "method": "GET",
        "header": [],
        "url": { "raw": "http://example.com/" }
      }
    },
    {
      "name": "Echo",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "body": { "mode": "raw", "raw": "{\"hello\":\"{{name}}\"}" },
        "url": { "raw": "http://example.com/echo" }
      }
    }
  ],
  "variable": [
    { "key": "name", "value": "Test" }
  ]
}
